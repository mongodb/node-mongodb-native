        {
            "auth": {
                "db": "admin",
                "password": "bar",
                "username": "bob"
            },
            "description": "User info for single IP literal host with database",
            "hosts": [
                {
                    "host": "::1",
                    "port": 27018,
                    "type": "ip_literal"
                }
            ],
            "options": null,
            "uri": "mongodb://bob:bar@[::1]:27018/admin",
            "valid": true,
            "warning": false
        },
        {
            "auth": {
                "db": null,
