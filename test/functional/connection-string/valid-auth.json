            "warning": false
        },
        {
            "auth": {
                "db": "admin",
                "password": "secret",
                "username": "alice"
            },
            "description": "User info for multiple hosts with database",
            "hosts": [
                {
                    "host": "example.com",
                    "port": null,
                    "type": "hostname"
                },
                {
                    "host": "::1",
                    "port": 27019,
                    "type": "ip_literal"
                }
