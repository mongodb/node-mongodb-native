{
  "description": "Standalone with default maxWireVersion of 0 is upgraded to one with maxWireVersion 21",
  "uri": "mongodb://a",
  "phases": [
    {
      "responses": [
        [
          "a:27017",
          {
            "ok": 1,
            "helloOk": true,
            "isWritablePrimary": true
          }
        ]
      ],
      "outcome": {
        "servers": {
          "a:27017": {
            "type": "Standalone",
            "setName": null
          }
        },
        "topologyType": "Single",
        "logicalSessionTimeoutMinutes": null,
        "setName": null,
        "compatible": false
      }
    },
    {
      "responses": [
        [
          "a:27017",
          {
            "ok": 1,
            "helloOk": true,
            "isWritablePrimary": true,
            "minWireVersion": 0,
            "maxWireVersion": 21
          }
        ]
      ],
      "outcome": {
        "servers": {
          "a:27017": {
            "type": "Standalone",
            "setName": null
          }
        },
        "topologyType": "Single",
        "logicalSessionTimeoutMinutes": null,
        "setName": null,
        "compatible": true
      }
    }
  ]
}
