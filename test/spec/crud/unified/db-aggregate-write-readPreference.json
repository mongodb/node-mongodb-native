                      "$listLocalSessions": {}
                    },
                    {
                      "$limit": 1
                    },
                    {
                      "$addFields": {
                        "_id": 1
                      }
                    },
                    {
                      "$project": {
                        "_id": 1
                      }
                    },
                    {
                      "$out": "coll0"
                    }
                  ],
                  "$readPreference": {
                    "mode": "secondaryPreferred",
                    "maxStalenessSeconds": 600
                  },
                  "readConcern": {
                    "level": "local"
                  },
                  "writeConcern": {
                    "w": 1
                  }
