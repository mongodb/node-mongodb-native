{
  "description": "findOne",
  "schemaVersion": "1.0",
  "createEntities": [
    {
      "client": {
        "id": "client0",
        "observeEvents": [
          "commandStartedEvent"
        ]
      }
    },
    {
      "database": {
        "id": "database0",
        "client": "client0",
        "databaseName": "find-tests"
      }
    },
    {
      "collection": {
        "id": "collection0",
        "database": "database0",
        "collectionName": "coll0"
      }
    }
  ],
  "initialData": [
    {
      "collectionName": "coll0",
      "databaseName": "find-tests",
      "documents": [
        {
          "_id": 1,
          "x": 11
        },
        {
          "_id": 2,
          "x": 22
        },
        {
          "_id": 3,
          "x": 33
        },
        {
          "_id": 4,
          "x": 44
        },
        {
          "_id": 5,
          "x": 55
        },
        {
          "_id": 6,
          "x": 66
        }
      ]
    }
  ],
  "tests": [
    {
      "description": "FindOne with filter",
      "operations": [
        {
          "object": "collection0",
          "name": "findOne",
          "arguments": {
            "filter": {
              "_id": 1
            }
          },
          "expectResult": {
            "_id": 1,
            "x": 11
          }
        }
      ],
      "expectEvents": [
        {
          "client": "client0",
          "events": [
            {
              "commandStartedEvent": {
                "command": {
                  "find": "coll0",
                  "filter": {
                    "_id": 1
                  },
                  "batchSize": {
                    "$$exists": false
                  },
                  "limit": 1,
                  "singleBatch": true
                },
                "commandName": "find",
                "databaseName": "find-tests"
              }
            }
          ]
        }
      ]
    },
    {
      "description": "FindOne with filter, sort, and skip",
      "operations": [
        {
          "object": "collection0",
          "name": "findOne",
          "arguments": {
            "filter": {
              "_id": {
                "$gt": 2
              }
            },
            "sort": {
              "_id": 1
            },
            "skip": 2
          },
          "expectResult": {
            "_id": 5,
            "x": 55
          }
        }
      ],
      "expectEvents": [
        {
          "client": "client0",
          "events": [
            {
              "commandStartedEvent": {
                "command": {
                  "find": "coll0",
                  "filter": {
                    "_id": {
                      "$gt": 2
                    }
                  },
                  "sort": {
                    "_id": 1
                  },
                  "skip": 2,
                  "batchSize": {
                    "$$exists": false
                  },
                  "limit": 1,
                  "singleBatch": true
                },
                "commandName": "find",
                "databaseName": "find-tests"
              }
            }
          ]
        }
      ]
    }
  ]
