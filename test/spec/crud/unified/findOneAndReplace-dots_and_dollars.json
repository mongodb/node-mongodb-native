    {
      "description": "Replacing document with top-level dotted key on 3.6+ server",
      "runOnRequirements": [
        {
          "minServerVersion": "3.6"
        }
      ],
      "operations": [
        {
          "name": "findOneAndReplace",
          "object": "collection0",
          "arguments": {
            "filter": {
              "_id": 1
            },
            "replacement": {
              "_id": 1,
              "a.b": 1
            }
          },
          "expectResult": {
            "_id": 1
          }
        }
      ],
      "expectEvents": [
        {
          "client": "client0",
          "events": [
            {
              "commandStartedEvent": {
                "command": {
                  "findAndModify": "coll0",
                  "query": {
                    "_id": 1
                  },
                  "update": {
                    "_id": 1,
                    "a.b": 1
                  },
                  "new": {
                    "$$unsetOrMatches": false
                  }
                }
              }
            }
          ]
        }
      ],
      "outcome": [
        {
          "collectionName": "coll0",
          "databaseName": "crud-tests",
          "documents": [
            {
              "_id": 1,
              "a.b": 1
            }
          ]
        }
      ]
