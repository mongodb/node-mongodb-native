              },
              {
                "name": "deleteOne",
                "arguments": {
                  "filter": {
                    "_id": 2
                  },
                  "hint": {
                    "_id": 1
                  }
                }
              }
            ],
            "options": {
              "ordered": true
            }
          }
        }
      ],
      "expectations": [
        {
          "command_started_event": {
            "command": {
              "delete": "BulkWrite_delete_hint",
              "deletes": [
                {
                  "q": {
                    "_id": 1
                  },
                  "hint": "_id_",
                  "limit": 1
                },
                {
                  "q": {
                    "_id": 2
                  },
                  "hint": {
                    "_id": 1
                  },
                  "limit": 1
                }
              ],
              "ordered": true
            }
          }
        }
      ],
      "outcome": {}
    },
    {
      "description": "Unacknowledged bulkWrite deleteMany with hints succeeds on server >= 4.4",
      "operations": [
        {
          "name": "bulkWrite",
          "collectionOptions": {
            "writeConcern": {
              "w": 0
            }
          },
          "arguments": {
            "requests": [
              {
                "name": "deleteMany",
                "arguments": {
                  "filter": {
                    "_id": {
                      "$lt": 3
                    }
                  },
                  "hint": "_id_"
                }
              },
              {
                "name": "deleteMany",
                "arguments": {
                  "filter": {
                    "_id": {
                      "$gte": 4
                    }
                  },
                  "hint": {
                    "_id": 1
                  }
