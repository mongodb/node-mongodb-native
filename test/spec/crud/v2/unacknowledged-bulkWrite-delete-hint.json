    {
      "description": "Unacknowledged bulkWrite deleteOne with hints succeeds on server >= 4.4",
      "operations": [
        {
          "name": "bulkWrite",
          "collectionOptions": {
            "writeConcern": {
              "w": 0
            }
          },
          "arguments": {
            "requests": [
              {
                "name": "deleteOne",
                "arguments": {
                  "filter": {
                    "_id": 1
                  },
                  "hint": "_id_"
