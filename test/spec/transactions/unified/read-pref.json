          "expectResult": [
            {
              "_id": 1
            },
            {
              "_id": 2
            },
            {
              "_id": 3
            },
            {
              "_id": 4
            }
          ]
        },
        {
          "object": "collection_rp_secondary",
          "name": "aggregate",
          "arguments": {
            "pipeline": [
              {
                "$project": {
                  "_id": 1
                }
              }
            ],
            "batchSize": 3,
            "session": "session0"
          },
          "expectResult": [
            {
              "_id": 1
            },
            {
              "_id": 2
            },
            {
              "_id": 3
