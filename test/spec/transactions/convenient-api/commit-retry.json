      "description": "commitTransaction succeeds after multiple connection errors",
      "failPoint": {
        "configureFailPoint": "failCommand",
        "mode": {
          "times": 2
        },
        "data": {
          "failCommands": [
            "commitTransaction"
          ],
          "closeConnection": true
        }
      },
      "operations": [
        {
          "name": "withTransaction",
          "object": "session0",
          "arguments": {
            "callback": {
              "operations": [
                {
                  "name": "insertOne",
                  "object": "collection",
                  "arguments": {
                    "session": "session0",
                    "document": {
                      "_id": 1
                    }
                  },
                  "result": {
                    "insertedId": 1
                  }
                }
              ]
            }
          }
        }
      ],
      "expectations": [
        {
          "command_started_event": {
            "command": {
              "insert": "test",
              "documents": [
                {
                  "_id": 1
                }
              ],
