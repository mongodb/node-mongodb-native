{
  "description": "redacted-commands",
  "schemaVersion": "1.5",
  "runOnRequirements": [
    {
      "minServerVersion": "5.0",
      "auth": false
    }
  ],
  "createEntities": [
    {
      "client": {
        "id": "client",
        "observeEvents": [
          "commandStartedEvent",
          "commandSucceededEvent"
        ],
        "observeSensitiveCommands": true
      }
    },
    {
      "database": {
        "id": "database",
        "client": "client",
        "databaseName": "command-monitoring-tests"
      }
    }
  ],
  "tests": [
    {
      "description": "authenticate",
      "operations": [
        {
          "name": "runCommand",
          "object": "database",
          "arguments": {
            "commandName": "authenticate",
            "command": {
              "authenticate": 1,
              "mechanism": "MONGODB-X509",
              "user": "CN=myName,OU=myOrgUnit,O=myOrg,L=myLocality,ST=myState,C=myCountry",
              "db": "$external"
            }
          },
          "expectError": {
            "isError": true
          }
        }
      ],
      "expectEvents": [
        {
          "client": "client",
          "events": [
            {
              "commandStartedEvent": {
                "commandName": "authenticate",
                "command": {
                  "authenticate": {
                    "$$exists": false
                  },
                  "mechanism": {
                    "$$exists": false
                  },
                  "user": {
                    "$$exists": false
                  },
                  "db": {
                    "$$exists": false
                  }
                }
              }
            }
          ]
        }
      ]
    },
